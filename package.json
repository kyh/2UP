{
  "name": "playhouse",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run dev:web\" \"npm run dev:voice\"",
    "dev:web": "npm run dev -w=web",
    "dev:voice": "npm run dev -w=voice",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.2"
  }
}
