{
  "name": "playhouse",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run gql-gen && react-scripts start",
    "build": "npm run codegen && react-scripts build",
    "format": "prettier --write .",
    "test": "react-scripts test",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "introspect-schema": "npx apollo schema:download --endpoint=http://localhost:4000/graphql schema.json",
    "codegen": "npx apollo codegen:generate --localSchemaFile=schema.json --target=typescript --includes=src/**/*.tsx --tagName=gql --addTypename --globalTypesFile=src/__generated__/graphql-types.ts __generated__",
    "gql-gen": "npm run introspect-schema && npm run codegen",
    "clean:codegen": "find . -name __generated__ -exec rm -rf {} \\;",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@reduxjs/toolkit": "^1.4.0",
    "apollo-boost": "^0.4.9",
    "lodash": "^4.17.19",
    "phoenix": "^1.5.4",
    "react": "^16.13.1",
    "react-alert": "^7.0.2",
    "react-beautiful-dnd": "^13.0.0",
    "react-canvas-draw": "^1.1.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.1.0",
    "react-modal": "^3.11.2",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux-logger": "^3.0.6",
    "reflexbox": "^4.0.6",
    "snyk": "^1.367.0",
    "styled-components": "^5.1.1",
    "styled-normalize": "^8.0.7",
    "uifx": "^2.0.7"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^5.3.19",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "@testing-library/user-event": "^12.0.15",
    "@types/jest": "^26.0.7",
    "@types/lodash": "^4.14.158",
    "@types/node": "^14.0.26",
    "@types/phoenix": "^1.5.0",
    "@types/react": "^16.9.43",
    "@types/react-alert": "^5.2.0",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-canvas-draw": "^1.1.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-modal": "^3.10.6",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/redux-logger": "^3.0.8",
    "@types/reflexbox": "^4.0.1",
    "@types/styled-components": "^5.1.1",
    "apollo": "^2.30.1",
    "babel-plugin-macros": "^2.8.0",
    "graphql": "^14.7.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "raw.macro": "^0.3.1",
    "react-scripts": "^3.4.1",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "snyk": true
}
