    "rehype-pretty-code": "^0.14.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "shiki": "^1.24.2"
  },
  "devDependencies": {
    "@kyh/eslint-config": "catalog:",
    "@kyh/prettier-config": "catalog:",
    "@kyh/tsconfig": "catalog:",
    "@types/mdx": "^2.0.13",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@kyh/prettier-config"
