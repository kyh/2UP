{
  "name": "@acme/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "postinstall": "prisma generate",
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "format": "prettier --check \"**/*.{mjs,ts,md,json}\"",
    "push": "pnpm with-env prisma db push",
    "studio": "pnpm with-env prisma studio",
    "seed": "pnpm with-env tsx prisma/seed.ts",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@prisma/extension-accelerate": "^0.6.2"
  },
  "devDependencies": {
    "@kyh/eslint-config": "*",
    "@kyh/prettier-config": "*",
    "@kyh/tsconfig": "*",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@kyh/eslint-config/base"
    ]
  },
  "prettier": "@kyh/prettier-config"
}
