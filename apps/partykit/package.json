{
  "name": "@2up/partykit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm with-env partykit dev",
    "deploy": "partykit deploy",
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "format": "prettier --check \"**/*.{mjs,ts,md,json}\"",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@2up/db": "workspace:*",
    "@2up/game": "workspace:*"
  },
  "devDependencies": {
    "@2up/tailwind-config": "workspace:*",
    "@kyh/eslint-config": "latest",
    "@kyh/prettier-config": "latest",
    "@kyh/tsconfig": "latest",
    "eslint": "^8.56.0",
    "partykit": "^0.0.66",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@kyh/eslint-config/base"
    ]
  },
  "prettier": "@kyh/prettier-config"
}
