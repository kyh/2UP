{
  "name": "@2up/partykit",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm with-env partykit dev",
    "deploy": "partykit deploy",
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "format": "prettier --check \"**/*.{mjs,ts,md,json}\"",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@2up/db": "workspace:*",
    "@2up/game": "workspace:*"
  },
  "devDependencies": {
    "@2up/eslint-config": "workspace:*",
    "@2up/prettier-config": "workspace:*",
    "@2up/tailwind-config": "workspace:*",
    "@2up/tsconfig": "workspace:*",
    "eslint": "^8.54.0",
    "partykit": "^0.0.39",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@2up/eslint-config/base"
    ]
  },
  "prettier": "@2up/prettier-config"
}
